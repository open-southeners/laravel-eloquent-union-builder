{
    "name": "open-southeners/laravel-eloquent-union-builder",
    "description": "Unify multiple Eloquent query builder instances into one using SQL union",
    "license": "MIT",
    "keywords": [
        "open-southeners",
        "eloquent",
        "laravel",
        "union-queries",
        "models",
        "laravel scout"
    ],
    "authors": [
        {
            "name": "Ruben Robles",
            "email": "me@d8vjork.com",
            "homepage": "https://d8vjork.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "laravel/scout": "^9.0",
        "nunomaduro/larastan": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^7.0|^9.0",
        "orchestra/testbench": "^4.0|^6.0|^7.0"
    },
    "suggest": {
        "laravel/scout": "Laravel Scout for use with UnionBuilder::search() method"
    },
    "autoload": {
        "psr-4": {
            "OpenSoutheners\\LaravelEloquentUnionBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenSoutheners\\LaravelEloquentUnionBuilder\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
